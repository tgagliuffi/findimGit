package com.bbva.findim.sql.dom;

public class TextoInformativoBean {
	private Integer idTextoInformativo;
	private String  descripcionTexto;
	
	public Integer getIdTextoInformativo() {
		return idTextoInformativo;
	}
	public void setIdTextoInformativo(Integer idTextoInformativo) {
		this.idTextoInformativo = idTextoInformativo;
	}
	public String getDescripcionTexto() {
		return descripcionTexto;
	}
	public void setDescripcionTexto(String descripcionTexto) {
		this.descripcionTexto = descripcionTexto;
	}
}
